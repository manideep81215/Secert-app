{
  "appId": "com.simpgames.quest",
  "appName": "Simp Games Quest",
  "webDir": "dist",
  "server": {
    "androidScheme": "https",
    "cleartext": true,
    "allowNavigation": [
      "https://secert-app.onrender.com",
      "wss://secert-app.onrender.com"
    ]
  },
  "plugins": {
    "CapacitorHttp": {
      "enabled": true
    },
    "Keyboard": {
      "resize": "native",
      "resizeOnFullScreen": true
    },
    "PushNotifications": {
      "presentationOptions": [
        "badge",
        "sound",
        "alert"
      ]
    }
  }
}
